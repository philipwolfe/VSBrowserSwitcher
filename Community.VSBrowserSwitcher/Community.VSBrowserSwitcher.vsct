				<Icon guid="guidImages" id="bmpFirefox" />
				<Strings>
					<CommandName>Firefox</CommandName>
					<ButtonText>Firefox</ButtonText>
				</Strings>
			</Button>

			<Button guid="guidIDEToolbarCmdSet" id="cmdidInternetExplorer" priority="0x0200" type="Button">
