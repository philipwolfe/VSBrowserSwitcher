			<Button guid="guidIDEToolbarCmdSet" id="cmdidOpera" priority="0x0500" type="Button">
				<Parent guid="guidIDEToolbarCmdSet" id="ToolbarGroup" />
				<Icon guid="guidImages" id="bmpOpera" />
