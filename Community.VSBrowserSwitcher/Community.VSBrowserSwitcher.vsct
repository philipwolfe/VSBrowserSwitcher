					<CommandName>Spartan</CommandName>
					<ButtonText>Spartan</ButtonText>
				</Strings>
			</Button>

		</Buttons>

		<Bitmaps>
			<Bitmap guid="guidImages" href="Resources\Toolbar.png" usedList="bmpFirefox, bmpIexplore, bmpChrome, bmpSafari, bmpOpera, bmpSpartan"/>

		</Bitmaps>

